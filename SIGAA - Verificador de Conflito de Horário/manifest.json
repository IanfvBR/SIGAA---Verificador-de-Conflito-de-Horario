{
  "manifest_version": 3,
  "name": "SIGAA - Verificador de Conflito de Horário",
  "short_name": "Verificador de Conflito",
  "version": "1.0",
  "description": "Dinamicamente sinaliza quais disciplinas causam ou não conflito de horário com as outras já selecionadas",
  "permissions": ["storage", "scripting","activeTab"],
  "action": {
  "default_title": "Verificar Conflitos"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "ConflitoHorarioSIGAA@ianfvbr.github.io",
      "strict_min_version": "142.0",

      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },

  "icons": {
  "16": "icons/icon16.png",
  "32": "icons/icon32.png",
  "48": "icons/icon48.png",
  "64": "icons/icon64.png",
  "96": "icons/icon96.png",
  "128": "icons/icon128.png"
  },

  "background": {
      "scripts": ["scripts/background-script.js"]
  }
}
